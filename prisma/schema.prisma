// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model StudentUser {
  id            Int @id @default(autoincrement())
  email         String
  first_name    String
  last_name     String
  major         String
}

model InstructorUser {
  id              Int @id @default(autoincrement())
  email           String
  first_name      String
  last_name       String
  field_of_study  String
}

model Post {
  id    Int    @id @default(autoincrement())
  title String
}

model Page {
  id    Int    @id @default(autoincrement())
  title String
}

model Tag {
  id           Int          @id @default(autoincrement())
  taggableId   Int
  taggableType String
}